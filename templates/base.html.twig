<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon"
          href="{{ asset('/images/logo/favourite_icon.png') }}">

    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        <!-- fraimwork - css include -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.min.css') }}">

        <!-- icon - css include -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/fontawesome.css') }}">

        <!-- animation - css include -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/aos.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/animate.css') }}">

        <!-- carousel - css include -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/slick.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/slick-theme.css') }}">

        <!-- popup - css include -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/magnific-popup.css') }}">

        <!-- select options - css include -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/nice-select.css') }}">

        <!-- pricing range - css include -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/jquery-ui.css') }}">

        <!-- custom - css include -->
        <link rel="stylesheet" type="text/css" href="{{ asset('css/style.css') }}">
    {% endblock %}

    {% block javascripts %}
        {#            {{ encore_entry_script_tags('app') }} #}
    {% endblock %}
</head>
<body>
<!-- backtotop - start -->
<div id="thetop"></div>
<div class="backtotop">
    <a href="#" class="scroll">
        <i class="fa fa-arrow-up"></i>
    </a>
</div>
<!-- backtotop - end -->

<!-- preloader - start -->
<div class="preloader">
    <div class="animation_preloader">
        <div class="spinner"></div>
        <p class="text-center">Loading</p>
    </div>
    <div class="loader">
        <div class="row vh-100">
            <div class="col-3 loader_section section-left">
                <div class="bg"></div>
            </div>
            <div class="col-3 loader_section section-left">
                <div class="bg"></div>
            </div>
            <div class="col-3 loader_section section-right">
                <div class="bg"></div>
            </div>
            <div class="col-3 loader_section section-right">
                <div class="bg"></div>
            </div>
        </div>
    </div>
</div>
<!-- preloader - end -->

<!-- header_section - start -->
<header class="header_section secondary_header sticky text-white clearfix">
    <div class="header_top clearfix">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-7">
                    <ul class="header_contact_info ul_li clearfix">
                        <li><i class="fal fa-envelope"></i> spartiates@email.com</li>
                        <li><i class="fal fa-phone"></i> +33-123-456-7890</li>
                    </ul>
                </div>
                <div class="col-lg-5">
                    <ul class="primary_social_links ul_li_right clearfix">
                        <li><a href="https://www.facebook.com/Amazon"><i class="fab fa-facebook-f"></i></a></li>
                        <li><a href="https://www.instagram.com/amazon/"><i class="fab fa-instagram"></i></a></li>
                        <li><a href="https://twitter.com/amazon"><i class="fab fa-twitter"></i></a></li>
                        <li><a href="https://www.youtube.com/user/AmazonWebServices/Cloud"><i
                                        class="fab fa-youtube"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="header_bottom clearfix">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-3 col-md-6 col-sm-6 col-6">
                    <div class="brand_logo">
                        <a href="{{ path('app_home') }}">
                            <img src="{{ asset('images/about/img_01.jpg') }}"
                                 srcset="{{ asset('images/logo/logo_01_2x.png') }}" alt="logo_not_found">
                            <img src="{{ asset('images/logo/logo_02_1x.png') }}"
                                 srcset="{{ asset('images/logo/logo_02_2x.png') }}" alt="logo_not_found">
                        </a>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 col-sm-6 col-6 order-last">
                    <ul class="header_action_btns ul_li_right clearfix">
                        <li class="dropdown">
                            <button type="button" class="user_btn" id="user_dropdown" data-toggle="dropdown"
                                    aria-haspopup="true" aria-expanded="false">
                                <i class="fa fa-user"></i>
                            </button>
                            <div class="user_dropdown rotors_dropdown dropdown-menu clearfix"
                                 aria-labelledby="user_dropdown">
                                <div class="profile_info clearfix">
                                    {# si l'utilisateur est connecté #}
                                    {% if app.user %}
                                        <div class="user_content">
                                            <h4 class="user_name">{{ app.user.firstname }} {{ app.user.lastname }}</h4>
                                        </div>
                                        <ul class="ul_li_block clearfix">
                                            <li><a href="#!"><i class="fa fa-user-circle"></i> Profile</a></li>
                                            <li><a href="#!"><i class="fa fa-user-cog"></i> Settings</a></li>
                                            <li><a href="#!"><i class="fa fa-sign-out"></i> Logout</a></li>
                                        </ul>

                                        {# si l'utilisateur est déconnecté #}
                                    {% else %}
                                        <div class="user_content">
                                            <h4 class="user_name">Welcome !</h4>
                                        </div>
                                        <ul class="ul_li_block clearfix">
                                            <li><a href="#!" class="text-dark"><i class="fa fa-address-card text-dark"></i> Subscribe</a></li>
                                            <li><a href="{{ path('app_user') }}" class="text-dark"><i class="fa fa-sign-in text-dark"></i> Login as User</a></li>
                                            <li><a href="{{ path('app_company') }}" class="text-dark"><i class="fa fa-sign-in text-dark"></i> Login as Company</a></li>
                                            <li><a href="/admin" class="text-dark"><i class="fa fa-sign-in text-dark"></i> Login as Amazon Staff</a>
                                            </li>
                                        </ul>
                                    {% endif %}
                                </div>


                            </div>
                        </li>
                        <li>
                            <button type="button" class="mobile_sidebar_btn"><i class="fa fa-align-right"></i>
                            </button>
                        </li>
                    </ul>
                </div>

                <div class="col-lg-6 col-md-12">
                    <nav class="main_menu clearfix">
                        <ul class="ul_li_center clearfix">
                            <li class="active has_child">
                                <a href="{{ path('app_home') }}">Home</a>
                                <ul class="submenu">
                                    <li><a href="{{ path('app_home') }}">Home Page</a></li>
                                </ul>
                            </li>
                            <li><a href="about.html">About</a></li>
                            <li class="has_child">
                                <a href="#!">Pages</a>
                                <ul class="submenu">
                                    <li><a href="service.html">Our Service</a></li>
                                    <li><a href="gallery.html">Car Gallery</a></li>
                                    <li><a href="account.html">My Account</a></li>
                                    <li><a href="reservation.html">Reservation</a></li>
                                    <li class="has_child">
                                        <a href="#!">Our Cars</a>
                                        <ul class="submenu">
                                            <li><a href="car.html">Cars</a></li>
                                            <li><a href="car_details.html">Car Details</a></li>
                                        </ul>
                                    </li>
                                    <li><a href="faq.html">F.A.Q.</a></li>
                                    <li><a href="login.html">Login</a></li>
                                </ul>
                            </li>
                            <li class="has_child">
                                <a href="#!">Contact Us</a>
                                <ul class="submenu">
                                    <li><a href="contact.html">Contact Default</a></li>
                                    <li><a href="contact_wordwide.html">Contact Wordwide</a></li>
                                </ul>
                            </li>
                        </ul>
                    </nav>
                </div>

            </div>
        </div>
    </div>

    <div id="collapse_search_body" class="collapse_search_body collapse">
        <div class="search_body">
            <div class="container">
                <form action="#">
                    <div class="form_item">
                        <input type="search" name="search" placeholder="Type here...">
                        <button type="submit"><i class="fa fa-search"></i></button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</header>

<!-- header_section - end
================================================== -->
{% block body %}{% endblock %}

<!-- footer_section - start
================================================== -->
<footer class="footer_section clearfix">
    <div class="footer_widget_area sec_ptb_100 clearfix" data-bg-color="#F2F2F2">
        <div class="container">
            <div class="row justify-content-lg-between">
                <div class="col-lg-4 col-md-4 col-sm-12 col-sm-12">
                    <div class="footer_about" data-aos="fade-up" data-aos-delay="100">
                        <div class="brand_logo mb_30">
                            <a href="index.html">
                                <img src="{{ asset('images/logo/logo_02_1x.png') }}"
                                     srcset="{{ asset('images/logo/logo_02_2x.png') }}" alt="logo_not_found">
                            </a>
                        </div>
                        <p class="mb_15">
                            You're a private? Going on vacation? Having a professional meeting? Moving house? Book right
                            now with Amazon Rental!
                        </p>
                        <p class="mb_15">
                            You're a company? Want to make profit out of your spare vehicles? Contact-us, we'll take
                            care of everything!
                        </p>
                        <div class="footer_useful_links mb_30">
                            <ul class="ul_li_block clearfix">
                                <li><a href="#!"><i class="fa fa-angle-right"></i> Rental Information</a></li>
                                <li><a href="#!"><i class="fa fa-angle-right"></i> F.A.Q.</a></li>
                            </ul>
                        </div>
                    </div>
                </div>

                <div class="col-lg-3 col-md-4 col-sm-12 col-sm-12">
                    <div class="footer_contact_info" data-aos="fade-up" data-aos-delay="200">
                        <h3 class="footer_widget_title">Contact Us:</h3>
                        <ul class="ul_li_block clearfix">
                            <li>
                                <strong><i class="fas fa-map-marker-alt"></i> Main Office Address:</strong>
                                <p class="mb-0">
                                    171 rue Lucien Faure, 33000 Bordeaux, France
                                </p>
                            </li>
                            <li><i class="fas fa-clock"></i> 9:00am-12:30pm / 2:00pm-05:30pm</li>
                            <li><i class="fas fa-envelope"></i> <strong>spartiates@eml.fr</strong></li>
                            <li><i class="fas fa-phone"></i> <strong>+33 123 456 7890</strong></li>
                        </ul>
                    </div>
                </div>

                <div class="col-lg-4 col-md-4 col-sm-12 col-sm-12">
                    <div class="footer_useful_links" data-aos="fade-up" data-aos-delay="300">
                        <h3 class="footer_widget_title">Information:</h3>
                        <ul class="ul_li_block clearfix">
                            <li><a href="#!"><i class="fa fa-angle-right"></i> Find a Car for Rent in the Nearest
                                    Location</a></li>
                            <li><a href="#!"><i class="fa fa-angle-right"></i> Cars Catalog</a></li>
                            <li><a href="#!"><i class="fa fa-angle-right"></i> F.A.Q.</a></li>
                            <li><a href="#!"><i class="fa fa-angle-right"></i> About Us</a></li>
                            <li><a href="#!"><i class="fa fa-angle-right"></i> Contact Us</a></li>
                            <li><a href="#!"><i class="fa fa-angle-right"></i> Help Center</a></li>
                            <li><a href="#!"><i class="fa fa-angle-right"></i> Privacy Police</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="footer_bottom text-white clearfix" data-bg-color="#000C21">
        <div class="container">
            <div class="row align-items-center justify-content-lg-between">
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                    <p class="copyright_text mb-2">Copyright © {{ "now"|date("Y") }} - Rotors template.</p>
                    <p class="copyright_text mb-0">Vehicle Rental by
                        <a class="author_links text-white"
                           href="https://github.com/JohannZych/AmazonRenting">Spartiate</a>
                        at WildCodeSchool, Bordeaux</p>
                </div>

                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                    <ul class="primary_social_links ul_li_right clearfix">
                        <li><a href="https://www.facebook.com/Amazon"><i class="fab fa-facebook-f"></i></a></li>
                        <li><a href="https://www.instagram.com/amazon/"><i class="fab fa-instagram"></i></a></li>
                        <li><a href="https://twitter.com/amazon"><i class="fab fa-twitter"></i></a></li>
                        <li><a href="https://www.youtube.com/user/AmazonWebServices/Cloud"><i
                                        class="fab fa-youtube"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</footer>

<!-- fraimwork - jquery include -->
<script src="{{ asset('js/jquery-3.5.1.min.js') }}"></script>
<script src="{{ asset('js/popper.min.js') }}"></script>
<script src="{{ asset('js/bootstrap.min.js') }}"></script>

<!-- animation - jquery include -->
<script src="{{ asset('js/aos.js') }}"></script>
<script src="{{ asset('js/parallaxie.js') }}"></script>

<!-- carousel - jquery include -->
<script src="{{ asset('js/slick.min.js') }}"></script>

<!-- popup - jquery include -->
<script src="{{ asset('js/magnific-popup.min.js') }}"></script>

<!-- select ontions - jquery include -->
